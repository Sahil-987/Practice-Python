   "source": [
    "# move zeros to the left\n",
    "# iow move non-zeros to right\n",
    "\n",
    "list_ = [0,1,0,2,0,5,6,0,7]\n",
    "\n",
    "\n",
    "lp = 0\n",
    "rp = 0\n",
    "\n",
    "for rp in range(len(list_)):\n",
    "    if list_[rp]:\n",
    "        # rp = non-zero\n",
    "        list_[rp], list_[lp] = list_[lp], list_[rp]\n",
    "        lp += 1\n",
    "\n",
    "list_"
   ]
